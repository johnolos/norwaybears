{
  "name": "norwaybears",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "remix vite:build && wrangler pages dev ./build/client",
    "build-cf-types": "wrangler types",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check --ignore-path .gitignore \"**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "@ark-ui/react": "^2.2.3",
    "@portabletext/react": "^3.0.15",
