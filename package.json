    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.19.2",
    "msw": "^2.2.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.2.5",
    "typescript": "^5.1.6",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.1",
